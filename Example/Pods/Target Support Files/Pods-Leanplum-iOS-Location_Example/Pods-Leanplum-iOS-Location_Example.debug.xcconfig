BITCODE_GENERATION_MODE = bitcode
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/Leanplum-iOS-Location" "${PODS_CONFIGURATION_BUILD_DIR}/Leanplum-iOS-SDK"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "${PODS_CONFIGURATION_BUILD_DIR}/Leanplum-iOS-Location/LeanplumLocation.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/Leanplum-iOS-SDK/Leanplum.framework/Headers"
OTHER_LDFLAGS = $(inherited) -ObjC -fembed-bitcode -framework "Leanplum" -framework "LeanplumLocation"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
